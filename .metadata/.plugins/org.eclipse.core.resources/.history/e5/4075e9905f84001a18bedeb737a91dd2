<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<button onclick = "myCar.addSpeed()">가속</button>
	<button onclick = "myCar.subSpeed()">감속</button>
	<script>
		let myCar = {	// [](대괄호)는 배열 {}(중괄호)는 클래스
				company : "현대자동차",	//키는 ""로 감싸도 되고 생략도 가능
				color : "red",			//값은 문자열일 경우 반드시 ""로 감싸야만 합니다.
				model : "그랜저",
				maxSpeed : 350,
				speed : 0;
				addSpeed : function() {
					this.speed += 10;
					console.log(speed);
				},
				subSpeed : function() {
					this.speed -= 10;
					console.log(speed);
				},
				moveSpeed : function(x) {
					this.speed += x;
					console.log(this.speed);
				}
				
		};
		
		console.log(myCar.company);
		console.log(myCar.color);
		console.log(myCar.speed);
		myCar.moveSpeed(100);
	</script>
</body>
</html>