	<script>
		function car() {	//메소드
			
		}
		
		function Car() {	//클래스
			this.company = "현대자동차",	
			this.color = "red",	
			this.model = "그랜저",
			this.maxSpeed = 350,
			this.speed = 0,
			this.addSpeed = function() {
				this.speed += 10;
				console.log(speed);
			},
			this.subSpeed = function() {
				this.speed -= 10;
				console.log(speed);
			},
			this.moveSpeed = function(x) {
				this.speed += x;
				console.log(this.speed);
			}
		}
		
		let myCar = new Car();
		console.log(myCar.company);
	</script>

