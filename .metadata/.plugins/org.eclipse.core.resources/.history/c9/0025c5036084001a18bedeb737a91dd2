<body>
	<script>
		function car() {	//메소드
			
		}
		
		function Car() {	//클래스
			company : "현대자동차",	
			color : "red",	
			model : "그랜저",
			maxSpeed : 350,
			speed : 0,
			addSpeed : function() {
				this.speed += 10;
				console.log(speed);
			},
			subSpeed : function() {
				this.speed -= 10;
				console.log(speed);
			},
			moveSpeed : function(x) {
				this.speed += x;
				console.log(this.speed);
			}
		}
		
		let myCar = new Car();
		console.log(myCar.company);
	</script>

