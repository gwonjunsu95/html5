<title>Insert title here</title>
</head>
<body>
	<script>
		function car() {	//메소드
			
		}
		
		function Car() {	//클래스
			company : "현대자동차",	//키는 ""로 감싸도 되고 생략도 가능
			color : "red",			//값은 문자열일 경우 반드시 ""로 감싸야만 합니다.
			model : "그랜저",
			maxSpeed : 350,
			speed : 0,
			addSpeed : function() {
				this.speed += 10;
				console.log(speed);
			},
			subSpeed : function() {
				this.speed -= 10;
				console.log(speed);
			},
			moveSpeed : function(x) {
				this.speed += x;
				console.log(this.speed);
			}
		}
	</script>

</body>
</html>